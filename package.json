  },
  "dependencies": {
    "elasticlunr": "^0.9.5",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eleventy-plugin-code-style-hooks": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.45.1"
  }
